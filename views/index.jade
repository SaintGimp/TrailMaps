extends layout

block body
  div.contentArea.tab-content(id='map_canvas')
    div.contents.tab-pane.active(id='bing-maps')
    div.contents.map-control.tab-pane(id='google-maps')
    // TODO: Here apparently supports canvas for hardware accel?
    div.contents.map-control.tab-pane(id='here-maps')

block scripts
  script(data-main='js/main', src='js/lib/require.js')
