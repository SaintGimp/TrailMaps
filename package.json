{
  "name": "TrailMaps",
  "version": "0.0.1",
  "description": "Maps for long-distance hiking trails",
  "private": true,
  "main": "server.js",
  "scripts": {
    "test": "npm run test:server",
    "test:server": "mocha test/server/*.js",
    "test:client": "echo 'Client tests require browser environment - see test/client/runner.html'",
    "test:all": "npm run lint && npm run test:server && npm run test:client",
    "lint": "eslint server.js routes domain data test public/js --ignore-pattern \"public/js/lib/**\" --ignore-pattern \"test/lib/**\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "node server.js",
    "dev": "node --watch --watch-preserve-output server.js"
  },
  "repository": "",
  "author": "Eric Lee",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0 <23.0.0",
    "npm": ">=10.0.0 <11.0.0"
  },
  "dependencies": {
    "applicationinsights": "^1.3.0",
    "body-parser": "^1.20.3",
    "coffeescript": "^2.7.0",
    "express": "^4.21.2",
    "express-ai": "^0.6.0",
    "less-middleware": "^3.1.0",
    "method-override": "^3.0.0",
    "mongodb": "^5.8.1",
    "morgan": "^1.10.1",
    "pug": "^3.0.3",
    "serve-favicon": "^2.5.0",
    "underscore": "^1.9.1",
    "xml2js": "^0.6.2"
  },
  "overrides": {
    "less": "^4.2.0",
    "form-data": "^4.0.0",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "eslint": "^5.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.0",
    "mocha": "^11.7.4",
    "sinon": "^7.3.2"
  }
}
