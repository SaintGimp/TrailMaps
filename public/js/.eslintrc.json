{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "2017",
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "amd": true,
        "jquery": true
    },
    "globals": {
        "requirejs": false
    },
    "rules": {
        "no-console": "error"
    }
}
